{
    "name": "Jupyter Notebooks Only",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    // "postCreateCommand": "conda env update --file ../.devcontainer/environment.yml --name venv",
    "postCreateCommand": "pip install -r ../requirements.txt",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    },
    "workspaceFolder": "/workspaces/Module1/notebooks",
    "forwardPorts": [8888],
    "remoteUser": "vscode"
  }
  