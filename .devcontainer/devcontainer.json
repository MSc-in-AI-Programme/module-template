{
    "name": "Jupyter Notebooks Only",
    "image": "mcr.microsoft.com/devcontainers/python:3.11",
    "features": {
      "ghcr.io/devcontainers/features/python:1": {
        "version": "3.11"
      },
      "ghcr.io/devcontainers/features/jupyterlab:1": {}
    },
    "workspaceFolder": "/workspaces/Module1/notebooks",
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    }
  }
  